IF
	InMyArea(Player1)
	OR(2)
		Class(Player1,CLERIC_ALL)
		Class(Player1,DRUID_ALL)
	Alignment(Player1,MASK_GOOD)
	!HasItem("AC#WSPKG",Player1)
	!GlobalTimerNotExpired("AC#W_PriestFail","GLOBAL")
THEN
	RESPONSE #100
		ApplySpellRES("AC#WCLG",Player1)  // Neutral gut
		Continue()
END

